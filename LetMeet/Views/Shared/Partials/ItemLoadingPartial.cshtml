@{
    Layout = "";
}

@model string

<link rel="stylesheet" href="~/css/Me/mainloadiersStyle.css">

    <div id="@Model-loading" class="main-loader-container " >
    <span class="loader se-pre-con"></span>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.js"></script>


    <script>
        $("#"+@Model).load(function () {
            // Animate loader off screen

        $("@Model-loading").fadeOut("slow");
        });
    </script>
